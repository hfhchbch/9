local Window = OrionLib:MakeWindow({
    Name = "脚本", 
    HidePremium = false, 
    SaveConfig = true,
    IntroText = "欢迎使用脚本", 
    ConfigFolder = "欢迎使用脚本"
})

local about = Window:MakeTab({
    Name = "Hub V10",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

about:AddParagraph("您的用户名:", " "..LocalPlayer.Name.."")
about:AddParagraph("您的注入器:", " "..identifyexecutor().."")
about:AddParagraph("您当前服务器的ID", " "..game.GameId.."")
	
local Tab = Window:MakeTab({
    Name = "通用脚本",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

Tab:AddButton({
    Name = "飞行",
    Callback = function()
        loadstring(game:HttpGet("https://pastebin.com/raw/41HyU1Hf"))()
    end
})

Tab:AddButton({
    Name = "无限跳",
    Callback = function()
        loadstring(game:HttpGet("https://pastebin.com/raw/V5PQy3y0", true))()
    end
})

Tab:AddButton({
    Name = "无敌少侠飞行",
    Callback = function()
        loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-Axhub-fly-74605"))()
    end
})

Tab:AddTextbox({
    Name = "移动速度",
    Default = "16",
    TextDisappear = true,
    Callback = function(Value)
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid.WalkSpeed = tonumber(Value) or 16
        end
    end	 
})

Tab:AddTextbox({
    Name = "跳跃高度",
    Default = "50",
    TextDisappear = true,
    Callback = function(Value)
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid.JumpPower = tonumber(Value) or 50
        end
    end	 
})

local Noclip = false
local Stepped = nil

Tab:AddToggle({
    Name = "穿墙",
    Default = false,
    Callback = function(Value)
        if Value then
            Noclip = true
            Stepped = game:GetService("RunService").Stepped:Connect(function()
                if Noclip and LocalPlayer.Character then
                    for _, child in pairs(LocalPlayer.Character:GetChildren()) do
                        if child:IsA("BasePart") then
                            child.CanCollide = false
                        end
                    end
                else
                    if Stepped then
                        Stepped:Disconnect()
                    end
                end
            end)
        else
            Noclip = false
            if Stepped then
                Stepped:Disconnect()
            end
        end
    end
})

Tab:AddToggle({
    Name = "夜视",
    Default = false,
    Callback = function(Value)
        if Value then
            game.Lighting.Ambient = Color3.new(1, 1, 1)
            game.Lighting.Brightness = 1
        else
            game.Lighting.Ambient = Color3.new(0, 0, 0)
            game.Lighting.Brightness = 0
        end
    end
})

local ExtraFeaturesTab = Window:MakeTab({
    Name = "透视",
    Icon = "rbxassetid://1234567890", 
    PremiumOnly = false
})

local lineXRayEnabled = false
local lineBeams = {}
local player = LocalPlayer
local camera = workspace.CurrentCamera

ExtraFeaturesTab:AddToggle({
    Name = "线条透视",
    Default = false,
    Callback = function(Value)
        if Value then
            lineXRayEnabled = true
            startLineXRay()
        else
            lineXRayEnabled = false
            stopLineXRay()
        end
    end
})

local function startLineXRay()
    stopLineXRay()
    local function createLines()
        if not player.Character or not camera then return end
        local character = player.Character
        local head = character:FindFirstChild("Head")
        local origin
        if head then
            origin = head.Position
        else
            local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
            if humanoidRootPart then
                origin = humanoidRootPart.Position
            else
                return
            end
        end
        local directions = {
            camera.CFrame.LookVector * 50,
            (camera.CFrame.LookVector + camera.CFrame.RightVector) * 50,
            (camera.CFrame.LookVector - camera.CFrame.RightVector) * 50,
            camera.CFrame.LookVector * 30 + Vector3.new(0, 30, 0),
            camera.CFrame.LookVector * 30 + Vector3.new(0, -30, 0),
            camera.CFrame.LookVector * 40 + camera.CFrame.RightVector * 20,
            camera.CFrame.LookVector * 40 - camera.CFrame.RightVector * 20,
        }
        local lineColor = Color3.new(0, 1, 0)
        for i, dir in ipairs(directions) do
            local target = origin + dir
            local attachment0 = Instance.new("Attachment")
            attachment0.Position = origin
            attachment0.Parent = workspace
            local attachment1 = Instance.new("Attachment")
            attachment1.Position = target
            attachment1.Parent = workspace
            local beam = Instance.new("Beam")
            beam.Name = "LineXRayBeam_" .. i
            beam.Attachment0 = attachment0
            beam.Attachment1 = attachment1
            beam.Width0 = 0.1
            beam.Width1 = 0.1
            beam.Color = ColorSequence.new(lineColor)
            beam.LightEmission = 1
            beam.LightInfluence = 0
            beam.Texture = "rbxassetid://2559882547"
            beam.TextureSpeed = 1
            beam.TextureScale = 1
            beam.Transparency = NumberSequence.new(0)
            beam.Parent = workspace
            table.insert(lineBeams, beam)
        end
    end
    game:GetService("RunService").RenderStepped:Connect(function()
        if lineXRayEnabled then
            createLines()
        end
    end)
end

local function stopLineXRay()
    for _, beam in pairs(lineBeams) do
        if beam and beam.Parent then
            beam:Destroy()
        end
    end
    lineBeams = {}
end

